	excludesfile = ~/.gitignore_global
        attributesfile = ~/.gitattributes_global
	pager = 

[diff "javaclass"]
      textconv = javap -c
      cachetextconv = true

[filter "c_code"]
        smudge = 
        clean = astyle < %f
