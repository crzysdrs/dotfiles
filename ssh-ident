   ("argv", "cs.pdx.edu",          "school"),
   ("argv", "crzysdrs.sytes.net",  "home"),
   ("argv", "blockade",            "home"),
   ("argv", "tooka",               "home"),
   ("argv", "slate",               "home"),
   ("argv", "github.com",          "github"),
   ("argv", "amazonaws",           "amazon"),
]
